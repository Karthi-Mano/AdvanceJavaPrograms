<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>Insert title here</title>
</head>
<body>
<script type="text/javascript">

var a=5;
var i=0;
while(i<=a)
	{
	  if(i%2==0)
		  {
	       document.writeln("Even value is\n"+i)
		  }
	  else
		  {
		  document.writeln("Odd value is\n"+i)
		  }
	  i=i+1;
	}

</script>
</body>
</html>