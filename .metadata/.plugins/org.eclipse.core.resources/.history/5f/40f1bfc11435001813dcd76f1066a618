<script>
function myMap() {
  var mapCanvas = document.getElementById("map");
  var mapOptions = {
    center: new google.maps.LatLng(10.9338334, 78.0883645), zoom: 10
  };
  var map = new google.maps.Map(mapCanvas, mapOptions);
}
</script>

